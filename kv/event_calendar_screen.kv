#:import event_calendar event_calendar

<EventCalendarScreen>:
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1)
        Rectangle:
            source: "icons/main/background.png"
            size: root.width, root.height
            pos: self.pos

    FloatLayout:

        FloatLayout:
            size_hint: 1, .12
            pos_hint: {"top": 1, "right": 1}

            FloatLayout:
                canvas.before:
                    Color:
                        rgba: (1, 1, 1, 1)
                    Rectangle:
                        source: "icons/events/panel.png"
                        size: self.width, self.height
                        pos: self.pos
                size_hint: .17, 1
                pos_hint: {"top": 1, "right": .18}

                ImageButton:
                    size_hint: .5, .5
                    pos_hint: {"top": .75, "right": .8}
                    source: "icons/events/back_arrow.png"
                    on_release:
                        app.change_screen(app.previous_screen)
                        app.previous_screen = "event_calendar_screen"

            FloatLayout:
                canvas.before:
                    Color:
                        rgba: (1, 1, 1, 1)
                    Rectangle:
                        source: "icons/events/panel.png"
                        size: self.width, self.height
                        pos: self.pos
                size_hint: .81, 1
                pos_hint: {"top": 1, "right": 1}

                ImageButton:
                    size_hint: .18, .6
                    pos_hint: {"top": .8, "right": .2}
                    source: "icons/calendar/month_back.png"
                    on_release:
                        event_calendar.month_back()
                Label:
                    id: month
                    text: ""
                    text_size: self.size
                    font_name: 'fonts/Nunito-Bold.ttf'
                    halign: 'center'
                    valign: 'middle'
                    font_size: '20sp'
                    pos_hint: {"top": 1, "right": 1}

                ImageButton:
                    size_hint: .18, .6
                    pos_hint: {"top": .8, "right": .95}
                    source: "icons/calendar/month_next.png"
                    on_release:
                        event_calendar.month_next()

        GridLayout:
            spacing: 1

            size_hint: 1, .88
            pos_hint: {"top": .88, "right": 1}
            id: calendar_layout
            name: "calendar_layout"
            cols: 7
            rows: 7
